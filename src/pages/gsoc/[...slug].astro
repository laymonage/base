---
import type { CollectionEntry } from 'astro:content';
import readingTime from 'reading-time';
import { getContentStaticPaths } from '../../content/config';
import BlogPost from '../../layouts/BlogPost.astro';

export async function getStaticPaths() {
  return await getContentStaticPaths('gsoc');
}
type Props = CollectionEntry<'gsoc'>;

const post = Astro.props;
---

<BlogPost {...post.data} readingTime={readingTime(post.rendered!.html).text}>
  <Fragment set:html={post.rendered!.html} />
</BlogPost>
